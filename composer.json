{
	"name": "mudandstars/historize-model-changes",
	"description": "This package allows for simple historization of specific columns of a model.",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Mudandstars\\HistorizeModelChanges\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Mudandstars\\HistorizeModelChanges\\Tests\\": "tests",
            "App\\": "vendor/orchestra/testbench-core/laravel/app/"
		},
		"classmap": [
			"tests",
            "vendor/orchestra/testbench-core/laravel/app/"
		]
	},
	"authors": [
		{
			"name": "Paul Sochiera",
			"email": "paul.sochiera@gmail.com"
		}
	],
	"require-dev": {
		"pestphp/pest": "^1.22",
		"pestphp/pest-plugin-laravel": "^1.4",
		"orchestra/testbench": "^8.0",
		"laravel/pint": "^1.4"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Mudandstars\\HistorizeModelChanges\\HMCServiceProvider"
			]
		}
	}
}
